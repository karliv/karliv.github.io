  "scripts": {
    "start": "cross-env NODE_ENV=production node --expose-gc server",
    "dev": "cross-env NODE_ENV=development node --expose-gc server",
    "prod": "cross-env NODE_ENV=production node --expose-gc server",
    "staging": "cross-env NODE_ENV=staging node --expose-gc server",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:server && npm run build:client && npm run assets",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:staging": "npm run clean && npm run build:server:staging && npm run build:client:staging && npm run assets",
    "build:client:staging": "cross-env NODE_ENV=staging webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server:staging": "cross-env NODE_ENV=staging webpack --config build/webpack.server.config.js --progress --hide-modules",
    "prewatch": "npm run build:staging",
    "watch": "npm-run-all --parallel watch:client watch:server watch:mon",
    "watch:client": "webpack --config build/webpack.client.config.js --watch --progress --hide-modules",
    "watch:server": "webpack --config build/webpack.server.config.js --watch --progress --hide-modules",
    "watch:mon": "nodemon \"./server.js\" --watch \"./dist\" --delay 10",
    "test": "standard && npm run dev",
    "assets": "mkdir -p dist && rsync -am --exclude='sass' ./src/assets/* dist",
    "translations": "node translations"
  },
